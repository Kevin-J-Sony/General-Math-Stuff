
def add(z1, z2):
    a = z1[0]
    b = z1[1]
    c = z2[0]
    d = z2[1]
    return (a+c,b+d)

def sub(z1, z2):
    a = z1[0]
    b = z1[1]
    c = z2[0]
    d = z2[1]
    return (a-c,b-d)

def mult(z1, z2):
    a = z1[0]
    b = z1[1]
    c = z2[0]
    d = z2[1]
    return (a*c-b*d,a*d+b*c)

def inv(z):
    a = z[0]
    b = z[1]
    return (a/(a**2 + b**2), -b/(a**2 + b**2))
